{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start HumaNet Development",
      "type": "shell",
      "command": "npm",
      "args": ["run", "dev"],
      "group": "build",
      "isBackground": true,
      "problemMatcher": []
    },
    {
      "label": "Build HumaNet",
      "type": "shell",
      "command": "npm",
      "args": ["run", "build"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": false,
      "problemMatcher": []
    },
    {
      "label": "Lint HumaNet",
      "type": "shell",
      "command": "npm",
      "args": ["run", "lint"],
      "group": "build",
      "isBackground": false,
      "problemMatcher": ["$eslint-stylish"]
    },
    {
      "label": "Format HumaNet Code",
      "type": "shell",
      "command": "npm",
      "args": ["run", "format"],
      "group": "build",
      "isBackground": false,
      "problemMatcher": []
    }
  ]
}
